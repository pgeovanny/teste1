  <!-- JS compacto: smooth scroll, contador e ano -->
  <script>
    // Smooth scroll para √¢ncoras internas
    document.querySelectorAll('a[href^="#"]').forEach(a=>{
      a.addEventListener('click', e=>{
        const id = a.getAttribute('href').slice(1);
        const el = document.getElementById(id);
        if(el){ e.preventDefault(); el.scrollIntoView({behavior:'smooth', block:'start'}); }
      });
    });

    // Atualiza ano no rodap√©
    document.getElementById('year').textContent = new Date().getFullYear();

    // Contador regressivo (edite data no #deadline[data-end])
    (function(){
      const d = document.getElementById('deadline');
      const t = document.getElementById('timer');
      if(!d || !t) return;
      const deadline = new Date(d.dataset.end);
      if(isNaN(deadline.getTime())) { t.textContent = 'em breve'; return; }
      function pad(n){return String(n).padStart(2,'0');}
      function tick(){
        const now = new Date();
        const ms = deadline - now;
        if(ms <= 0){ t.textContent = 'encerrada'; return; }
        const s = Math.floor(ms/1000)%60;
        const m = Math.floor(ms/60000)%60;
        const h = Math.floor(ms/3600000)%24;
        const dleft = Math.floor(ms/86400000);
        t.textContent = (dleft>0? dleft+'d ':'')+pad(h)+':'+pad(m)+':'+pad(s);
        requestAnimationFrame(tick);
      }
      tick();
    })();
  </script>
